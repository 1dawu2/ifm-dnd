{
    "id": "ifm.dnd",
    "version": "0.0.1",
    "name": "IFM Drag&Drop",
    "description": "IFM Drag&Drop",
    "newInstancePrefix": "IFM",
    "icon": "https://1dawu2.github.io/ifm-dnd/icon.png",
    "vendor": "IFM",
    "eula": "",
    "license": "",
    "imports": [
        "input-controls"
    ],
    "webcomponents": [
        {
            "kind": "main",
            "tag": "ifm-dnd",
            "url": "https://1dawu2.github.io/ifm-dnd/ifm-dnd.js",
            "integrity": "sha384-2LDjBTrg2pXRLv0bpPBMb/V1HflDRh5GqF4uCvZagiyJD7G0Unstv8c8c6pHJP0m",
            "ignoreIntegrity": false
        }
    ],
    "properties": {
        "metadata": {
            "type": "string",
            "description": "For internal use",
            "default": ""
        },
        "list": {
            "type": "ListItem[]",
            "description": "list",
            "default": [
                {
                    "id": "item1",
                    "description": "description 1",
                    "iconFile": "sap-icon://shipping-status"
                }
            ]
        }
    },
    "methods": {
        "getList": {
            "returnType": "ListItem[]",
            "description": "Get list",
            "parameters": [],
            "body": "return this.list;"
        },
        "setList": {
            "description": "Set list",
            "parameters": [
                {
                    "name": "value",
                    "type": "ListItem[]",
                    "description": "The new list"
                }
            ],
            "body": "this.list = value;"
        }
    },
    "types": {
        "ListItem": {
            "description": "A list item",
            "properties": [
                {
                    "name": "id",
                    "type": "string",
                    "description": "ID of the list item",
                    "mandatory": true
                },
                {
                    "name": "description",
                    "type": "string",
                    "description": "Description of the list item",
                    "mandatory": false
                },
                {
                    "name": "iconFile",
                    "type": "string",
                    "description": "Icon file name of the list item",
                    "mandatory": false
                }
            ]
        }
    },
    "events": {
        "onStart": {
            "description": "onStart events trigger"
        }
    }
}